    "using JuMP\n",
    "using MIPMatrixTools.IPInstances\n",
    "\n",
    "model = Model()\n",
    "C = rand(1:10, 2, 3)\n",
    "@variable(model, x[1:3] >= 0, Int)\n",
    "@objective(model, Min, C * x)\n",
    "@constraint(model, sum(x) <= 10)\n",
    "\n",
    "instance = IPInstance(model)\n",
    "@show C\n",
    "@show instance.C"
   ]
